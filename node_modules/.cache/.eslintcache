],"/Users/tyama/Desktop/programming/Develop/UOH_syllabus_search/uoh_syllabus/src/reportWebVitals.js",[],"/Users/tyama/Desktop/programming/Develop/UOH_syllabus_search/uoh_syllabus/src/App.jsx",["29","30"],"import { React, useState, useRef } from \"react\";\nimport './App.css';\nimport { SyllabusPage } from \"./components/SyllabusPage\";\nimport { SyllabusSearch } from \"./components/SyllabusSearch/SyllabusSearch\";\n\n\nexport const App = () => {\n\n\n  return (\n    <>\n      <SyllabusSearch />\n      <SyllabusPage />\n    </>\n  )\n};\n","/Users/tyama/Desktop/programming/Develop/UOH_syllabus_search/uoh_syllabus/src/components/SyllabusPage.jsx",[],"/Users/tyama/Desktop/programming/Develop/UOH_syllabus_search/uoh_syllabus/src/components/SyllabusSearch/SyllabusSearch.jsx",["31"],"import { React, useState, useRef } from \"react\";\nimport './SyllabusSearch.css';\nimport logoImage from \"../../data/UOH_Database_logo_mini.png\";\nimport { Lecture } from \"./Lecture\"\nimport Syllabus from \"../../data/syllabus_map.json\";\n\n\nexport const SyllabusSearch = () => {\n  const [searchText, setSearchText] = useState(\"\");\n  const textRef = useRef(null)\n  // チェックボックス\n  const initialVal = { common: false, sis: false, em: false, emgbc: false, eng:false, sci:false, shse:false, cnas:false, minor:false, cons:false, 1:false, 2:false, 3:false, 4:false, first:false, second:false};\n  const [val, setVal] = useState(initialVal);\n\n  // JSONデータ\n  const data = Syllabus;\n  // 検索ボタン\n  const onClickSearch = () => {\n    setSearchText(textRef.current.value);\n  };\n\n  const onClickReset = () => {\n    setSearchText(\"\")\n  };\n\n  const handleChange = e => {\n    const newVal = Object.assign({}, val, {\n      [e.target.value]: !val[e.target.value]\n    });\n    setVal(newVal);\n  };\n\n  return (\n    <>\n      <div className=\"header\">\n        <h1>UOH DATABASE</h1>\n      </div>\n      <div className=\"search-form\">\n        <div className=\"check-box-1\">\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"common\"\n              onChange={handleChange}\n              checked={val['common']}\n            />\n            全学共通科目\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"sis\"\n              onChange={handleChange}\n              checked={val['sis']}\n            />\n            社会情報科学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"em\"\n              onChange={handleChange}\n              checked={val['em']}\n            />\n            国際商経学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"emgbc\"\n              onChange={handleChange}\n              checked={val['emgbc']}\n            />\n            国際商経GCB\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"eng\"\n              onChange={handleChange}\n              checked={val['eng']}\n            />\n          工学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"sci\"\n              onChange={handleChange}\n              checked={val['sci']}\n            />\n            理学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"shse\"\n              onChange={handleChange}\n              checked={val['shse']}\n            />\n          環境人間学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"cnas\"\n              onChange={handleChange}\n              checked={val['cnas']}\n            />\n            看護学部\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"minor\"\n              onChange={handleChange}\n              checked={val['minor']}\n            />\n          副専攻\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"cons\"\n              onChange={handleChange}\n              checked={val['cons']}\n            />\n          集中講義\n          </label>\n        </div>\n        <div className=\"year\">\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"1\"\n              onChange={handleChange}\n              checked={val[\"1\"]}\n            />\n          1年次\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"2\"\n              onChange={handleChange}\n              checked={val[\"2\"]}\n            />\n          2年次\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"3\"\n              onChange={handleChange}\n              checked={val[\"3\"]}\n            />\n          3年次\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"4\"\n              onChange={handleChange}\n              checked={val[\"4\"]}\n            />\n          4年次\n          </label>\n        </div>\n        <div className=\"term\">\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"first\"\n              onChange={handleChange}\n              checked={val[\"first\"]}\n            />\n          前期\n          </label>\n          <label>\n            <input\n              type=\"checkbox\"\n              value=\"second\"\n              onChange={handleChange}\n              checked={val[\"second\"]}\n            />\n          後期\n          </label>\n        </div>\n        <div className=\"input-area\">\n          <input ref={textRef} placeholder=\" 科目名を部分一致検索\" className=\"text-input\" />\n          <button className=\"input-button\" onClick={onClickSearch}>検索</button>\n          <button className=\"reset-button\" onClick={onClickReset}>リセット</button>\n        </div>\n\n      </div>\n      <table>\n        <thead>\n          <tr>\n            <th className=\"lecture-name\">科目名</th><th>開講年次・学期</th><th>担当教員</th><th>科目番号</th>\n          </tr>\n        </thead>\n        <tbody>\n          {Object.keys(data).map(key => <Lecture key={key} details={data[key]} filterClass={val} searchText={searchText} />)}\n        </tbody>\n    </table>\n    </>\n  )\n};\n","/Users/tyama/Desktop/programming/Develop/UOH_syllabus_search/uoh_syllabus/src/components/SyllabusSearch/Lecture.jsx",["32"],"import React from 'react';\n\nexport const Lecture = (props) => {\n  const { details } = props;\n  const { filterClass } = props;\n  const { searchText } = props;\n  // レンダリングするかどうか\n  const flag_array = Object.keys(filterClass).filter((output, index) => {\n    return filterClass[output] && details.class.includes(output)\n  });\n  const count_array = Object.values(filterClass).filter((output, index) => {\n    return output === true\n  });\n  const searchFlag = details.name.includes(searchText);\n\n  return (\n    <>\n      { searchFlag && count_array.length === flag_array.length && <tr><td>{details.name}　<a className={details.class} href={details.url} target=\"_blank\"><i className=\"far fa-file-alt icon\"></i></a></td><td>{details.year_term}</td><td>{details.professor_name}</td><td>{details.key}</td></tr>}\n    </>\n  );\n};\n",{"ruleId":"33","severity":1,"message":"34","line":1,"column":17,"nodeType":"35","messageId":"36","endLine":1,"endColumn":25},{"ruleId":"33","severity":1,"message":"37","line":1,"column":27,"nodeType":"35","messageId":"36","endLine":1,"endColumn":33},{"ruleId":"33","severity":1,"message":"38","line":3,"column":8,"nodeType":"35","messageId":"36","endLine":3,"endColumn":17},{"ruleId":"39","severity":1,"message":"40","line":18,"column":90,"nodeType":"41","messageId":"42","endLine":18,"endColumn":154,"fix":"43"},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'useRef' is defined but never used.","'logoImage' is defined but never used.","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a security risk in older browsers: see https://mathiasbynens.github.io/rel-noopener/#recommendations","JSXOpeningElement","noTargetBlankWithoutNoreferrer",{"range":"44","text":"45"},[658,658]," rel=\"noreferrer\""]